    "import numpy as np\n",
    "\n",
    "markov = np.array([[0.9, 0.3], [0.1, 0.7]])\n",
    "state = np.array([1.0, 0.0])\n",
    "\n",
    "for i in range(100):\n",
    "    state = np.matmul(markov, state)\n",
    "print(state)\n",
    "\n",
    "markov_n = markov.copy()\n",
    "for i in range(100):\n",
    "    markov_n = np.matmul(markov_n, markov)\n",
    "print(markov_n)"
